.box{
  height: 50px;
  
}
.navlist{
  width: calc(25% - 2px);
  float: left;
  line-height: 50px;
  text-align: center;
  /* border: 1px solid #ccc; */
}

page{
  background-color: #F0F2F5;
}
.pro{
  margin:10px 10px;
}

.pro>.item{
  float: left;
  width: calc(50% - 5px);
  background-color: #fff;
  margin-bottom: 10px;
}

.pro>.item.a{
  margin-right: 5px;
}

.pro>.item.b{
  margin-left: 5px;
}

.pro .pro-info{
  padding: 5px;
}

.pro .pro-name{
  font-size: 12px;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.pro .price-box{
  margin-top: 5px;
}

.pro .price{
  font-size: 14px;
  color: #e4393c;
}

.pro .pri{
  line-height: 23px;
}

.pro .buy{
  padding: 1px;
  border: 1px solid #e4393c;
  margin-left: 5px;
}

.pro .same{
  padding: 1px;
  border: 1px solid #ddd;
  border-radius: 5px;
  font-size: 12px;
  color: #8a8a8a;
}

.vip{
  font-size: 14px;
  color: #e49; 
  height: 19px;
  margin-top: 5px;
}

.not-data{
  color: #8a8a8a;
  text-align: center;
  margin: 10px auto;
  font-size: 12px;
}

.like{
  width: 19px;
}

.selected{
  color: red;
  border-bottom: 1px solid red;
  background-color: #f0f0f0;
}